{
  "name": "ngwmn-ui",
  "version": "0.1.0dev",
  "description": "National Ground Water Monitoring Network",
  "scripts": {
    "test": "karma start",
    "test:chrome": "karma start --browsers Chrome",
    "test:firefox": "karma start --browsers Firefox",
    "test:watch": "karma start --debug --no-single-run --auto-watch --browsers Chrome",
    "build": "run-p build:* && npm run hash-assets",
    "build:js": "mkdir -p dist/scripts && rollup -c --environment NODE_ENV:production",
    "hash-assets": "gulp --gulpfile ./gulpfile.js revall",
    "serve:static": "live-server --port=9000 --cors --no-browser assets/dist",
    "serve:python": "env/bin/python run.py",
    "start": "run-p watch serve:python",
    "watch": "run-p watch:* serve:static",
    "watch:js": "mkdir -p dist/scripts && rollup -c --watch"
  },
  "engines": {
    "node": "8.11.3"
  },
  "respository": {
    "type": "git",
    "url": "git+https://github.com/ACWI-SSWD/ngwmn-ui"
  },
  "author": "USGS",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/ACWI-SSWD/ngwmn-ui/issues"
  },
  "homepage": "https://github.com/ACWI-SSWD/ngwmn-ui#readme",
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "eslint": "^4.19.1",
    "eslint-plugin-jasmine": "^2.9.2",
    "gulp": "^3.9.1",
    "gulp-rev-all": "^0.9.7",
    "jasmine": "^2.99.0",
    "jasmine-ajax": "^3.3.1",
    "karma": "^2.0.2",
    "karma-browserify": "^5.2.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-ajax": "^0.1.13",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0",
    "rollup": "^0.59.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-handlebars-plus": "^0.2.3",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "dependencies": {
    "autotrack": "^2.4.1",
    "disc": "^1.3.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  }
}
