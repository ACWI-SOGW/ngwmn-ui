{
  "name": "ngwmn-ui",
  "version": "0.16.0dev",
  "description": "National Ground Water Monitoring Network",
  "scripts": {
    "test": "karma start",
    "test:chrome": "karma start --browsers Chrome",
    "test:firefox": "karma start --browsers Firefox",
    "test:watch": "karma start --debug --no-single-run --auto-watch --browsers Chrome",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && run-p build:* && npm run hash-assets",
    "build:autotrack": "terser --output dist/autotrack.js node_modules/autotrack/autotrack.js",
    "build:css": "sass --load-path node_modules/uswds/src/stylesheets src/styles/main.scss dist/main.css && postcss dist/main.css -map -o dist/main.css",
    "build:fonts": "mkdir -p dist/fonts && cp -r node_modules/uswds/dist/fonts/* dist/fonts && cp node_modules/@fortawesome/fontawesome-free/webfonts/* dist/fonts",
    "build:images": "mkdir -p dist/img && mkdir -p dist/img && cp -r src/images/* dist/img && cp -r node_modules/uswds/dist/img/* dist/img",
    "build:patterns": "mkdir -p dist/patterns && cp src/patterns/* dist/patterns",
    "build:js": "mkdir -p dist/scripts && rollup -c --environment NODE_ENV:production",
    "hash-assets": "gulp --gulpfile ./gulpfile.js revall",
    "serve:static": "live-server --port=9000 --cors --no-browser dist",
    "serve:python": "env/bin/python run.py",
    "start": "run-p watch serve:python",
    "watch": "run-p watch:* serve:static",
    "watch:css": "nodemon -w src/styles -e scss -x \"npm run build:css\"",
    "watch:fonts": "npm run build:fonts",
    "watch:images": "npm run build:images",
    "watch:patterns": "npm run build:patterns",
    "watch:js": "rollup -c --watch"
  },
  "engines": {
    "node": "14.15.1"
  },
  "respository": {
    "type": "git",
    "url": "git+https://github.com/ACWI-SSWD/ngwmn-ui"
  },
  "author": "USGS",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/ACWI-SSWD/ngwmn-ui/issues"
  },
  "homepage": "https://github.com/ACWI-SSWD/ngwmn-ui#readme",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "autoprefixer": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-plugin-jasmine": "^4.1.1",
    "gulp": "^4.0.2",
    "gulp-rev-all": "^2.0.3",
    "jasmine": "^3.6.3",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-rollup-preprocessor": "^7.0.5",
    "karma-spec-reporter": "^0.0.32",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.0",
    "postcss-cli": "^8.3.0",
    "postcss-csso": "^5.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "redux-mock-store": "^1.5.4",
    "rollup": "^2.34.2",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.36.0",
    "stylelint": "^13.8.0",
    "stylelint-scss": "^3.18.0",
    "svgo": "^1.3.2",
    "terser": "^5.5.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.1",
    "@juggle/resize-observer": "3.2.0",
    "autotrack": "2.4.1",
    "d3": "5.16.0",
    "fast-memoize": "2.5.2",
    "list.js": "2.3.0",
    "lodash": "4.17.20",
    "raf-throttle": "^2.0.5",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "uswds": "2.9.0"
  }
}
