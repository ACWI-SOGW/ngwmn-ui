{
  "name": "ngwmn-ui",
  "version": "0.2.0dev",
  "description": "National Ground Water Monitoring Network",
  "scripts": {
    "test": "karma start",
    "test:chrome": "karma start --browsers Chrome",
    "test:firefox": "karma start --browsers Firefox",
    "test:watch": "karma start --debug --no-single-run --auto-watch --browsers Chrome",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && run-p build:* && npm run hash-assets",
    "build:autotrack": "uglifyjs --output dist/autotrack.js node_modules/autotrack/autotrack.js",
    "build:css": "node-sass --include-path node_modules/uswds/dist/scss src/styles/main.scss dist/main.css && uglifycss --output dist/main.css dist/main.css && postcss dist/main.css --no-map --use autoprefixer postcss-flexbugs-fixes -o dist/main.css",
    "build:fonts": "mkdir -p dist/fonts && cp -r node_modules/uswds/dist/fonts/* dist/fonts && cp node_modules/@fortawesome/fontawesome-free/webfonts/* dist/fonts",
    "build:images": "mkdir -p dist/img && mkdir -p dist/img && cp -r src/images/* dist/img && cp -r node_modules/uswds/dist/img/* dist/img",
    "build:patterns": "mkdir -p dist/patterns && cp src/patterns/* dist/patterns",
    "build:js": "mkdir -p dist/scripts && rollup -c --environment NODE_ENV:production",
    "hash-assets": "gulp --gulpfile ./gulpfile.js revall",
    "serve:static": "live-server --port=9000 --cors --no-browser dist",
    "serve:python": "env/bin/python run.py",
    "start": "run-p watch serve:python",
    "watch": "run-p watch:* serve:static",
    "watch:css": "nodemon -w src/styles -e scss -x \"npm run build:css\"",
    "watch:fonts": "npm run build:fonts",
    "watch:images": "npm run build:images",
    "watch:patterns": "npm run build:patterns",
    "watch:js": "rollup -c --watch"
  },
  "engines": {
    "node": "10.13.0"
  },
  "respository": {
    "type": "git",
    "url": "git+https://github.com/ACWI-SSWD/ngwmn-ui"
  },
  "author": "USGS",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/ACWI-SSWD/ngwmn-ui/issues"
  },
  "homepage": "https://github.com/ACWI-SSWD/ngwmn-ui#readme",
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "eslint": "^4.19.1",
    "eslint-plugin-jasmine": "^2.9.2",
    "gulp": "^3.9.1",
    "gulp-rev-all": "^1.0.0",
    "jasmine": "^2.99.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-rollup-preprocessor": "^6.1.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0",
    "postcss-flexbugs-fixes": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "rollup": "^0.66.6",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "svgo": "^1.1.1",
    "uglify-js": "^3.4.9",
    "uglifycss": "0.0.29"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "autotrack": "^2.4.1",
    "d3": "^5.5.0",
    "fast-memoize": "^2.5.1",
    "list.js": "^1.5.0",
    "lodash": "^4.17.11",
    "raf-throttle": "^2.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.0",
    "uswds": "^2.0.0-beta.3"
  }
}
